class Factorial { 
long factorial(int number) { 
        long result = 1; 
        for (int i = 1; i <= number; i++) { 
            result *= i; 
        } 
        return result; 
    } 
    public static void main(String args[]) { 
        int number = 5; 
        long result = factorial(number); 
        System.out.println("Factorial of " + number + " is: " + result); 
    } 
}
